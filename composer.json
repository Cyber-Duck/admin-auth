{
    "name": "brackets/admin-auth",
    "description": "Authentication interface for brackets/admin-ui",
    "keywords": [
        "auth",
        "authentication",
        "login",
        "user",
        "admin",
        "cms",
        "laravel"
    ],
    "license": "MIT",
    "type": "project",
    "authors": [
        {
            "name": "Pavol Perdík",
            "email": "pavol.perdik@brackets.sk",
            "homepage": "https://www.brackets.sk",
            "role": "Developer"
        },
        {
            "name": "David Běhal",
            "email": "david.behal@brackets.sk",
            "homepage": "https://www.brackets.sk",
            "role": "Developer"
        }
