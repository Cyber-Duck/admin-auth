    "require": {
        "php": "^8.3",
        "brackets/admin-ui": "^1.0",
        "brackets/media": "^1.0",
        "illuminate/support": "^11.0",
        "spatie/laravel-permission": "^6.0"
    },
    "require-dev": {
        "orchestra/testbench": "^9.0",
        "mockery/mockery": "^1.6",
        "phpunit/phpunit": "^10.0"
